{
  "duration": {
    "instant": "0ms",
    "fast": "150ms",
    "normal": "300ms",
    "slow": "500ms",
    "slower": "750ms"
  },
  "easing": {
    "linear": "linear",
    "ease": "ease",
    "easeIn": "ease-in",
    "easeOut": "ease-out",
    "easeInOut": "ease-in-out",
    "bounce": "cubic-bezier(0.68, -0.55, 0.265, 1.55)",
    "smooth": "cubic-bezier(0.4, 0, 0.2, 1)"
  },
  "keyframes": {
    "fadeIn": {
      "0%": { "opacity": "0" },
      "100%": { "opacity": "1" }
    },
    "fadeOut": {
      "0%": { "opacity": "1" },
      "100%": { "opacity": "0" }
    },
    "slideUp": {
      "0%": { "transform": "translateY(100%)" },
      "100%": { "transform": "translateY(0)" }
    },
    "slideDown": {
      "0%": { "transform": "translateY(-100%)" },
      "100%": { "transform": "translateY(0)" }
    },
    "scaleIn": {
      "0%": { "transform": "scale(0.95)", "opacity": "0" },
      "100%": { "transform": "scale(1)", "opacity": "1" }
    }
  }
}